
node {
     stage('Build image') {
         app = docker.build("884868906286.dkr.ecr.ap-northeast-2.amazonaws.com/test")
     }
s
     stage('Push image') {
         docker.withRegistry('https://884868906286.dkr.ecr.ap-northeast-2.amazonaws.com', 'ecr:ap-northeast-2:ecr-aws-plugin') {
             app.push("${env.BUILD_NUMBER}")
             app.push("latest")
     }
  }
}
